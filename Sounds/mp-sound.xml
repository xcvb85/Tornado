		<engine1-wake-forward>
			<name>Left Engine Wake</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado/Sounds/wake.wav</path>
			<condition>
				<and>
					<not>
						<property>/sim/current-view/internal</property>
					</not>
					<or>
						<equals>
							<property>/sim/current-view/name</property>
							<value>Fly-By View</value>
						</equals>
						<equals>
							<property>/sim/current-view/name</property>
							<value>Tower View</value>
						</equals>
						<equals>
							<property>/sim/current-view/name</property>
							<value>Tower View Look From</value>
						</equals>
					</or>
				</and>
			</condition>
			<position>
				<x>25</x>
				<y>-0.5</y>
				<z>0</z>
			</position>
			<orientation>
				<x>1</x>
				<y>0</y>
				<z>0</z>
				<inner-angle>15.0</inner-angle>
				<outer-angle>90.0</outer-angle>
				<outer-gain>0.025</outer-gain>
			</orientation>
			<volume>
				<property>velocities/mach</property>
				<factor>1.9</factor>
				<offset>-0.3</offset>
				<min>0.00</min>
				<max>1.0</max>
			</volume>
		</engine1-wake-forward>
