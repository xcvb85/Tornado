		<!-- flyby -->
		<flyby>
			<name>fly-by</name>
			<mode>once</mode>
			<path>Aircraft/Tornado/Sounds/flyby.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>velocities/mach</property>
						<value>0.8</value>
					</greater-than>
					<equals>
						<property>sim/current-view/view-number</property>
						<value>6</value>
					</equals>
				</and>
				<not>
					<property>sim/current-view/internal</property>
				</not>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
		</flyby>
		<!-- Sonic Boom -->
		<sonicboom>
			<name>Sonic Boom</name>
			<mode>once</mode>
			<path>Aircraft/Tornado/Sounds/sonicboom.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>velocities/mach</property>
						<value>1.0</value>
					</greater-than>
					<equals>
						<property>sim/current-view/internal</property>
						<value>false</value>
					</equals>
				</and>
			</condition>
		</sonicboom>
		<guns>
			<name>gunfire</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado/Sounds/m61.wav</path>
			<condition>
				<property>controls/armament/trigger</property>
				<property>ai/submodels/submodel/count</property>
			</condition>
			<volume>
				<factor>1.2</factor>
				<max>1.0</max>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<reference-dist>600.0</reference-dist>
			<max-dist>1000.0</max-dist>
		</guns>
