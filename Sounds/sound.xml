			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1.25</offset>
			</pitch>
		</wind>
		<sonicboom>
			<name>Sonic Boom</name>
			<mode>once</mode>
			<path>Aircraft/Tornado/Sounds/misc/sonicboom.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>velocities/mach</property>
						<value>1.0</value>
					</greater-than>
					<equals>
						<property>/sim/current-view/internal</property>
						<value>false</value>
					</equals>
				</and>
			</condition>
		</sonicboom>
		<voice>
			<name>Geforce-breath-vox</name>
			<path>Aircraft/Tornado/Sounds/misc/Geforce.wav</path>
			<condition>
				<greater-than>
					<property>/accelerations/pilot/z-accel-fps_sec</property>
					<value>-161</value>
				</greater-than>
			</condition>
			<volume>
				<factor>0.9</factor>
			</volume>
		</voice>
		<voice>
			<name>Geforce-vox</name>
			<path>Aircraft/Tornado/Sounds/misc/geforce1.wav</path>
