		<!--================================= avionics =============================-->
		<cockpit>
			<name>Cockpit Atmos</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado/Sounds/misc/cockpit.wav</path>
			<condition>
				<property>/sim/current-view/internal</property>
			</condition>
			<volume>
				<offset>1</offset>
				<min>0.2</min>
				<max>1.0</max>
			</volume>
		</cockpit>
		<stall>
			<name>stall</name>
			<mode>looped</mode>
			<path>/Aircraft/Tornado/Sounds/misc/stall.wav</path>
			<condition>
				<less-than>
					<property>/orientation/alpha-deg</property>
					<value>15</value>
				</less-than>
				<not>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
					<property>surface-positions/flap-pos-norm</property>
				</not>
			</condition>
			<volume>
				<max>0.5</max>
			</volume>
			<position>
				<x>-22</x>
				<y>0</y>
				<z>0</z>
			</position>
			<reference-dist>1.0</reference-dist>
			<max-dist>2.0</max-dist>
		</stall>

		<!--================================= misc =============================-->
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>/velocities/airspeed-kt</property>
